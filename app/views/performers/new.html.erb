<%= form_for @performer, url: { action: "create" } do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <div class="field">
    <%= f.label :soundcloud_url %>
    <%= f.text_field :soundcloud_url %>
  </div>
  <div class="actions">
    <%= f.submit :List %>
  </div>
<% end %>
